{

    "appenders": [{
            "type": "console",
            "category": "console"
        },
        {
            "category": "app",
            "type": "file",
            "filename": "./logs/log_file/file.log",
            "maxLogSize": 104800,
            "backups": 10
        },
        {
            "category": "http",
            "type": "dateFile",
            "filename": "./logs/log_date/date",
            "alwaysIncludePattern": true,
            "pattern": "-yyyy-MM-dd-hh.log"
        }
    ],
    "replaceConsole": false
}